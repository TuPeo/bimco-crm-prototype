{
  "name": "bimco-storybook",
  "engines": {
    "node": ">=20.17.0 <21"
  },
  "scripts": {
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:css lint:js",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:css:fix lint:js:fix",
    "lint:css": "stylelint \"stories/**/*.{css,scss}\" --cache --cache-location .cache/.stylelintcache",
    "lint:css:fix": "npm run lint:css -- --fix",
    "lint:js": "eslint \"stories/**/*.{js,ts,tsx}\" --cache --cache-location .cache/.eslintcache",
    "lint:js:fix": "npm run lint:js -- --fix",
    "storybook": "storybook dev -p 6006",
    "test-storybook": "test-storybook",
    "build-storybook": "storybook build"
  },
  "author": "Cantarus",
  "license": "ISC",
